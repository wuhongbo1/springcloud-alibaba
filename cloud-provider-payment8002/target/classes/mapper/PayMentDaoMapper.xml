<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wu.springcloud.dao.PayMentDao">
    <insert id="create" useGeneratedKeys="true" keyProperty="id">
        insert into payment(id,serial) values (null,#{serial})
    </insert>
    <resultMap id="BaseResultMap" type="com.wu.springcloud.entities.PayMent">
        <id column="id" property="id"></id>
        <result column="serial" property="serial"></result>
    </resultMap>
    <select id="findById" resultMap="BaseResultMap">
        select * from payment where id = #{id}
    </select>
</mapper>